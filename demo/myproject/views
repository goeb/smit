+smv 1.1.0-rc5
addView "Deleted issues" \
    filterin status deleted \

addView "Open issues" \
    default \
    filterin status open \
    sort +mtime \

addView "test xxx-abb-renaed" \
    search toto

addView ttty \
    filterin ctime "" \
    filterin summary "" \
    filterout summary "" \
    colspec id+ctime+status \
    search ttty

