+smv 1.1.0-rc4
addView "Deleted issues" \
    filterin status deleted \

addView "Open issues" \
    default \
    filterin status open \
    sort +mtime \

addView "test xxx-abb" \
    filterin status open \
    filterin status closed \
    filterout owner "" \
    sort -mtime+id \
    colspec id+mtime+summary \
    search toto

addView ttty \
    search ttty

